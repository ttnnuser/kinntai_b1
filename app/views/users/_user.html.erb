<li>
  <%= link_to user.name, user %><br>
  <% if current_user.admin? && !current_user?(user) %>
    | <%= link_to "削除", user, method: :delete, date: { confirm: "削除してもよろしいですか？" }, class: "btn btn-primary" %><br>
    | <%= link_to "編集", basic_info_path(user), class: "btn btn-primary" %>
  <% else %>
    | <%= link_to "編集", basic_info_path(user), class: "btn btn-primary" %>
  <% end %>
</li>